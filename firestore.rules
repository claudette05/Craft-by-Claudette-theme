rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    // Allow public read access for all documents,
    // but only allow write access for the admin user.
    match /{document=**} {
      allow read: if true;
      allow write: if request.auth.uid == 'Im7W3p0tpNX68ljdRgBHSxZJ3X63';
    }
  }
}
